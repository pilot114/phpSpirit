{
    "name": "symfony/ai-platform",
    "description": "PHP library for interacting with AI platform provider.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "ai",
        "aimlapi",
        "albert",
        "anthropic",
        "azure",
        "bedrock",
        "cerebras",
        "dockermodelrunner",
        "elevenlabs",
        "gemini",
        "huggingface",
        "inference",
        "litellm",
        "llama",
        "lmstudio",
        "meta",
        "mistral",
        "nova",
        "ollama",
        "openai",
        "openrouter",
        "perplexity",
        "replicate",
        "transformers",
        "vertexai",
        "voyage"
    ],
    "authors": [
        {
            "name": "Christopher Hertel",
            "email": "mail@christopher-hertel.de"
        },
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-fileinfo": "*",
        "oskarstark/enum-helper": "^1.5",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^2.1",
        "psr/log": "^3.0",
        "symfony/clock": "^7.3|^8.0",
        "symfony/event-dispatcher": "^7.3|^8.0",
        "symfony/property-access": "^7.3|^8.0",
        "symfony/property-info": "^7.3|^8.0",
        "symfony/serializer": "^7.3|^8.0",
        "symfony/type-info": "^7.3|^8.0",
        "symfony/uid": "^7.3|^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.17",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.5.46",
        "symfony/cache": "^7.3|^8.0",
        "symfony/console": "^7.3|^8.0",
        "symfony/dotenv": "^7.3|^8.0",
        "symfony/expression-language": "^7.3|^8.0",
        "symfony/finder": "^7.3|^8.0",
        "symfony/http-client": "^7.3|^8.0",
        "symfony/http-client-contracts": "^3.5",
        "symfony/process": "^7.3|^8.0",
        "symfony/var-dumper": "^7.3|^8.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Platform\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\AI\\Agent\\Tests\\": "../agent/tests/",
            "Symfony\\AI\\Fixtures\\": "../../fixtures",
            "Symfony\\AI\\PHPStan\\": "../../.phpstan/",
            "Symfony\\AI\\Platform\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        },
        "thanks": {
            "name": "symfony/ai",
            "url": "https://github.com/symfony/ai"
        }
    }
}